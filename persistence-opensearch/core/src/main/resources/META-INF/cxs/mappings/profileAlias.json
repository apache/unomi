{
  "dynamic_templates": [
    {
      "all": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "analyzer": "folding",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        }
      }
    }
  ],
  "properties": {
    "creationDate" : {
      "type" : "date"
    },
    "lastModificationDate" : {
      "type" : "date"
    },
    "lastSyncDate" : {
      "type" : "date"
    },
    "creationTime": {
      "type": "date"
    },
    "modifiedTime": {
      "type": "date"
    }
  }
}
