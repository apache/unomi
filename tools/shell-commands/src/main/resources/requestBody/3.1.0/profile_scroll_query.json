{
  "size": 100,
  "query": {
    "bool": {
      "must": [
        {
          "match": {
            "itemType": "profile"
          }
        }
      ],
      "filter": [
        {
          "exists": {
            "field": "properties.nbOfVisits"
          }
        }
      ]
    }
  },
  "_source": ["itemId"]
}

