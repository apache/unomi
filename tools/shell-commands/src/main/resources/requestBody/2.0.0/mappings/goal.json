{
  "dynamic_templates": [
    {
      "all": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "analyzer": "folding",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        }
      }
    }
  ],
  "properties": {
    "creationDate" : {
      "type" : "date"
    },
    "lastModificationDate" : {
      "type" : "date"
    },
    "lastSyncDate" : {
      "type" : "date"
    },
    "metadata": {
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "hidden": {
          "type": "boolean"
        },
        "missingPlugins": {
          "type": "boolean"
        },
        "readOnly": {
          "type": "boolean"
        }
      }
    },
    "startEvent": {
      "type": "object",
      "enabled": false
    },
    "targetEvent": {
      "type": "object",
      "enabled": false
    }
  }
}
