{
  "$id": "https://unomi.apache.org/schemas/json/consent/1-0-0",
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "self":{
    "vendor":"org.apache.unomi",
    "name": "consent",
    "format":"jsonschema",
    "version":"1-0-0"
  },
  "title": "Consent",
  "type": "object",
  "properties" : {
    "scope" : {
      "type" : "string",
      "validateScope" : true
    },
    "typeIdentifier" : {
      "type" : "string"
    },
    "status" : {
      "enum" : [ "GRANTED", "DENIED", "REVOKED" ]
    },
    "statusDate" : {
      "type" : "string",
      "format" : "date-time"
    },
    "revokeDate" : {
      "type" : "string",
      "format" : "date-time"
    }
  },
  "unevaluatedProperties": false
}
