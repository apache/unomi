{
  "dynamic_templates": [
    {
      "all": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "analyzer": "folding",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        }
      }
    }
  ],
  "properties": {
    "creationDate": {
      "type": "date"
    },
    "lastModificationDate": {
      "type": "date"
    },
    "properties": {
      "type": "object",
      "enabled": true
    },
    "apiKeys": {
      "type": "nested",
      "properties": {
        "expirationDate": {
          "type": "date"
        },
        "creationDate": {
          "type": "date"
        }
      }
    }
  }
}
