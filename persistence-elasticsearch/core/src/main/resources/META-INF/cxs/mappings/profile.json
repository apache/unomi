{
  "dynamic_templates": [
    {
      "all": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "analyzer": "folding",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        }
      }
    }
  ],
  "properties": {
    "creationDate" : {
      "type" : "date"
    },
    "lastModificationDate" : {
      "type" : "date"
    },
    "lastSyncDate" : {
      "type" : "date"
    },
    "properties": {
      "properties": {
        "age": {
          "type": "long"
        },
        "firstVisit": {
          "type": "date"
        },
        "lastVisit": {
          "type": "date"
        },
        "previousVisit": {
          "type": "date"
        },
        "nbOfVisits": {
          "type": "long"
        },
        "interests": {
          "type": "nested"
        }
      }
    },
    "systemProperties": {
      "properties": {
        "pastEvents": {
          "type": "nested"
        }
      }
    },
    "consents": {
      "properties": {
        "statusDate": {
          "type": "date"
        },
        "revokeDate": {
          "type": "date"
        }
      }
    }
  }
}
